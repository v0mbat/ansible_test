upstream 34.245.103.122 {
    server 172.31.10.181;
    }
proxy_cache_path /var/www/cache levels=1:2 keys_zone=cache:30m max_size=1G;
proxy_ignore_headers Expires Cache-Control;

server {
    listen 80;
    server_name 34.245.103.122;
    location / {
	if ($http_cookie ~* ".+" ) {
	    set $do_not_cache 1;
	}
	proxy_cache_bypass $do_not_cache;
	proxy_pass http://172.31.10.181:80;
	proxy_cache cache;
	proxy_cache_valid 404 502 503 1m;
	proxy_cache_valid any 1h;
	
	proxy_set_header Host 34.245.103.122;
	#proxy_set_header X-Real-IP ;
	#proxy_set_header X-Forwarded-For 172.31.10.181;
	proxy_connect_timeout 120;
	proxy_send_timeout 120;
	proxy_read_timeout 180;
    }
}